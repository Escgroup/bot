{
  "name": "esc-bot",
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "dev": "nodemon --watch src --ext ts --exec ts-node --require dotenv/config src/index",
    "lint": "eslint --ext ts .",
    "fix": "eslint --ext ts --fix ."
